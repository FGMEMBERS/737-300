  </action>
  
 <action>  <name>course decrease lh</name>  <button>0</button>  <x>-120</x>  <y>-20</y>  <w>15</w>  <h>15</h>  <binding>   <command>property-adjust</command>   <property>/instrumentation/nav[0]/radials/selected-deg</property>   <step>-1.0</step>   <min>0</min>   <max>360</max>   <wrap>1</wrap>  </binding> </action>
 
