<?xml version="1.0"?>
 
<PropertyList>

 <fx>

   <leftstartup>
	 <name>leftenginestart</name>
	 <path>/Aircraft/737-300/Sounds/engine-start.wav</path>
	 <mode>once</mode>
	 <condition>
       <property>/controls/engines/engine[0]/starter</property>
	 </condition>
	 <volume>
	   <factor>1</factor>
	 </volume>
   </leftstartup>
	 
   <rightstartup>
	 <name>rightenginestart</name>
	 <path>/Aircraft/737-300/Sounds/engine-start.wav</path>
	 <mode>once</mode>
	 <condition>
       <property>/controls/engines/engine[1]/starter</property>
	 </condition>
	 <volume>
	   <factor>1</factor>
	 </volume>
   </rightstartup>

	 	 
   <engine>
   <name>leftengine</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>ln</type>
    <factor>0.10</factor>
    <offset>0.05</offset>
   </volume>
<position>
<x>4.5</x>
<y>4.7</y>
<z>1.2</z>
</position>
			<orientation>
				<x> 0 </x>
				<y> 1 </y>
				<z> 0 </z>
				<inner-angle>180</inner-angle>
				<outer-angle>360</outer-angle>
				<outer-gain>0.1</outer-gain>
        <reference-dist>20</reference-dist>
			</orientation>
  </engine>
  
  <engine>
   <name>leftengine</name>
   <path>/Aircraft/737-300/Sounds/thrust.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>ln</type>
    <factor>0.40</factor>
    <offset>0.05</offset>
   </volume>
<position>
<x>7.35</x>
<y>4.7</y>
<z>1.2</z>
</position>
			<orientation>
				<x> 1 </x>
				<y> 0 </y>
				<z> 0 </z>
				<inner-angle>90</inner-angle>
				<outer-angle>180</outer-angle>
				<outer-gain>0.75</outer-gain>
        <reference-dist>20</reference-dist>
			</orientation>
  </engine>
  
  <engine>
   <name>rightengine</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[1]/running</property>     
   </condition>
   <volume>
    <property>/engines/engine[1]/thrust_lb</property>
    <type>ln</type>
    <factor>0.10</factor>
    <offset>0.05</offset>
   </volume>
<position>
<x>4.5</x>
<y>-4.7</y>
<z>1.2</z>
</position>
			<orientation>
				<x> 0 </x>
				<y> 1 </y>
				<z> 0 </z>
				<inner-angle>180</inner-angle>
				<outer-angle>360</outer-angle>
				<outer-gain>0.1</outer-gain>
        <reference-dist>20</reference-dist>
			</orientation>
  </engine>
  
  <engine>
   <name>rightengine</name>
   <path>/Aircraft/737-300/Sounds/thrust.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[1]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[1]/thrust_lb[0]</property>
    <type>ln</type>
    <factor>0.40</factor>
    <offset>0.05</offset>
   </volume>
<position>
<x>7.35</x>
<y>-4.7</y>
<z>1.2</z>
</position>
			<orientation>
				<x> 1 </x>
				<y> 0 </y>
				<z> 0 </z>
				<inner-angle>90</inner-angle>
				<outer-angle>180</outer-angle>
				<outer-gain>0.75</outer-gain>
				<reference-dist>20</reference-dist>
			</orientation>
  </engine>

  <engine>
   <name>leftwhine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[0]/running</property>
   </condition>
   <volume>
    <property>/engines/engine[0]/thrust_lb[0]</property>
    <type>ln</type>
    <factor>0.05</factor>
    <offset>0.1</offset>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thrust_lb[0]</property>
    <type>log</type>
    <factor>0.10</factor>
    <offset>0.10</offset>
   </pitch>
	 <position>
	   <x>4.5</x>
	   <y>4.7</y>
		 <z>1.2</z>
	 </position>
		<orientation>
			<x> -1 </x>
			<y> 0 </y>
			<z> 0 </z>
			<inner-angle>90</inner-angle>
			<outer-angle>180</outer-angle>
			<outer-gain>0.1</outer-gain>
		  <reference-dist>10</reference-dist>
		</orientation>
  </engine>
  
  <engine>
   <name>rightwhine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
     <property>/engines/engine[1]/running</property>     
   </condition>
   <volume>
    <property>/engines/engine[1]/thrust_lb[0]</property>
    <type>ln</type>
    <factor>0.05</factor>
    <offset>0.1</offset>
   </volume> 
   <pitch>
    <property>/engines/engine[1]/thrust_lb[0]</property>
    <type>log</type>
    <factor>0.10</factor>
    <offset>0.10</offset>
   </pitch> 
	 <position>
	   <x>4.5</x>
		 <y>-4.7</y>
		 <z>1.2</z>
	 </position>
		<orientation>
			<x> -1 </x>
			<y> 0 </y>
			<z> 0 </z>
			<inner-angle>90</inner-angle>
			<outer-angle>180</outer-angle>
			<outer-gain>0.1</outer-gain>
			<reference-dist>10</reference-dist>
		</orientation>
  </engine>


  <rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>Aircraft/737-300/Sounds/roll.wav</path>
   <condition>
	   <and>
	     <greater-than>
	       <property>/velocities/groundspeed-kt</property>
				 <value>9</value>
	     </greater-than>
       <or>
         <property>/gear/gear[0]/wow</property>
         <property>/gear/gear[1]/wow</property>
         <property>/gear/gear[2]/wow</property>
       </or>
		 </and>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.5</factor>
   </volume>
  </rumble>

  <gear>
   <name>gear</name>
   <mode>in-transit</mode>
   <path>Sounds/gear.wav</path>
   <property>/gear/gear[0]/position-norm</property>
   <pitch>
    <internal>dt_play</internal>
    <offset>1.0</offset>
    <factor>-0.001</factor>
   </pitch>
  </gear>

  <gear>
   <name>gear-lock</name>
   <path>Sounds/gear-lck.wav</path>
   <condition>
    <equals>
     <property>/gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
   </condition>
  </gear>


  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <flap>
   <name>flaps</name>
   <mode>in-transit</mode>
   <path>Sounds/flaps.wav</path>
   <property>/surface-positions/flap-pos-norm</property>
   <volume>
    <factor>0.45</factor>
   </volume>
  </flap>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind-8khz.wav</path>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/position/altitude-ft</property>
    <factor>-0.000010</factor>
    <offset>1.0</offset>
    <min>0.1</min>
    <max>1.0</max>
   </volume>
   <volume>
     <property>/velocities/airspeed-kt</property>
     <factor>0.0010</factor>
     <min>0.03</min>
     <max>0.25</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>
 
  <stall>
   <name>stall</name>
	 <mode>looped</mode>
   <path>Aircraft/737-300/Sounds/stall.wav</path>
   <condition>
     <and>
       <greater-than>
         <property>/fdm/jsbsim/aero/alpha-deg</property>
	       <value>12.0</value>
	     </greater-than>
	     <greater-than>
	       <property>/velocities/airspeed-kt</property>
	       <value>20</value>
	     </greater-than>
	   </and>
	 </condition>
   <volume>
	   <factor>1</factor>
   </volume>
  </stall>

  <overspeed>
   <name>overspeed</name>
	 <mode>looped</mode>
   <path>Aircraft/737-300/Sounds/overspeed.wav</path>
  <condition>
  <or>
    <greater-than>
      <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
	    <value>340</value>
	  </greater-than>
	  <greater-than>
	    <property>/instrumentation/airspeed-indicator/indicated-mach</property>
	    <value>0.82</value>
	  </greater-than>
	</or>
	</condition>
  <volume>
    <factor>1</factor>
  </volume>
  </overspeed>
  
	<overalt>
   <name>overalt</name>
	 <mode>looped</mode>
   <path>Aircraft/737-300/Sounds/overspeed.wav</path>
  <condition>
    <greater-than>
      <property>/instrumentation/altimeter/indicated-altitude-ft</property>
	    <value>37009</value>
	  </greater-than>
	</condition>
  <volume>
    <factor>1</factor>
  </volume>
  </overalt>

  <trimming>
    <name>trimming</name>
    <mode>once</mode>
    <path>Aircraft/737-300/Sounds/trim.wav</path>
    <condition>
      <greater-than>
        <property>/b733/sound/stab-trim</property>
        <value>0.01</value>
      </greater-than>
    </condition>
    <volume>
      <factor>1</factor>
    </volume>
  </trimming>

  <alt-acq>
    <name>alt-acq</name>
    <mode>once</mode>
    <path>Aircraft/737-300/Sounds/alt-acq.wav</path>
    <condition>
      <property>/b733/sound/alt-acq</property>
    </condition>
    <volume>
      <factor>1</factor>
    </volume>
  </alt-acq>
	
	
	<seatbelt-on>
		<name>seatbelt-sign-on</name>
		<mode>once</mode>
		<path>Aircraft/737-300/Sounds/seatbelt.wav</path>
		<condition>
			  <property>/controls/switches/seatbelt-sign</property>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</seatbelt-on>

	<seatbelt-off>
		<name>seatbelt-sign-off</name>
		<mode>once</mode>
		<path>Aircraft/737-300/Sounds/seatbelt.wav</path>
		<condition>
			  <not>
			    <property>/controls/switches/seatbelt-sign</property>
			  </not>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</seatbelt-off>

 	<nosmoke-on>
		<name>nosmoking-sign-on</name>
		<mode>once</mode>
		<path>Aircraft/737-300/Sounds/seatbelt.wav</path>
		<condition>
			  <property>/controls/switches/nosmoking-sign</property>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</nosmoke-on>
	
 	<nosmoke-off>
		<name>nosmoking-sign-off</name>
		<mode>once</mode>
		<path>Aircraft/737-300/Sounds/seatbelt.wav</path>
		<condition>
			  <not>
			    <property>/controls/switches/nosmoking-sign</property>
			  </not>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</nosmoke-off>

</fx>
 
</PropertyList>
