<PropertyList>
<sim>
		<description>Boeing 737-300</description>
		<author>David Culp (FDM), Innis Cunningham(3d), Heiko Schulz (3D and Panel), Emmanuel Baranger(3d)</author>
		<status>early-production</status>

		<flight-model>jsb</flight-model>
		<aero>737-300</aero>




<panel>
		<path>Aircraft/737-300/Panels/737-ifr-panel.xml</path>
		<visibility>true</visibility>
</panel>
    
		<model>
			<path>Aircraft/737-300/Models/737-300.xml</path>
			<pushback>
  <kp type="double">100</kp>
  <ki type="double">25</ki>
  <kd type="double">0</kd>
  <position-norm type="double">0</position-norm>
 </pushback>
  <livery>  
    <file type="string">733UA</file>
   </livery> 
</model>
		
    
		<view>
			<internal archive="y">true</internal>
			<config>
				<!-- x/y/z == right/up/back -->
				<x-offset-m archive="y">-0.475</x-offset-m>
				<y-offset-m archive="y">1.85</y-offset-m>
				<z-offset-m archive="y">2.830</z-offset-m>
<!--				<z-offset-m archive="y">-6.300</z-offset-m>-->
				<pitch-offset-deg archive="y">-11.0</pitch-offset-deg>
        <default-field-of-view-deg type="double">60.0</default-field-of-view-deg>
        <dynamic>
        <enabled type="bool" userarchive="y">false</enabled>
        </dynamic>
			</config>
		</view>
    
		<view n="1">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>
    
		<view n="2">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>

		<view n="3">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>

		<view n="4">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>

		<view n="5">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>

		<view n="6">
			<config>
				<target-z-offset-m archive="y" type="double">16.859</target-z-offset-m>
			</config>
		</view>
    
		 		
    
		<chase-distance-m>-50</chase-distance-m>

		<flaps>
			<setting>0.000</setting>
			<!-- flaps up -->
			<setting>0.125</setting>
			<!-- flaps 1  -->
			<setting>0.250</setting>
			<!-- flaps 2  -->
			<setting>0.375</setting>
			<!-- flaps 5  -->
			<setting>0.500</setting>
			<!-- flaps 10 -->
			<setting>0.625</setting>
			<!-- flaps 15 -->
			<setting>0.750</setting>
			<!-- flaps 25 -->
			<setting>0.875</setting>
			<!-- flaps 30 -->
			<setting>1.000</setting>
			<!-- flaps 40 -->
		</flaps>
    
 		<!--<ai>
			<submodels>
				<contrails type="bool">true</contrails>
			</submodels>
		</ai>-->
  
    <systems>
      <electrical>
        <path></path>
      </electrical>
      <autopilot>
	      <path>Aircraft/737-300/737-autopilotV5.xml</path>
      </autopilot>
    </systems>
    <sound>
     <path>Aircraft/737-300/Sounds/737-sound.xml</path>
	 <switch type = "bool"></switch>
    </sound>
    <hud>
      <enable3d>false</enable3d>
    </hud>
    <virtual-cockpit archive="y">true</virtual-cockpit>
    <allow-toggle-cockpit>true</allow-toggle-cockpit>
  
<help>
 <title>Boeing 737-300</title>
 <line>the take off precedure for a 737 is to have flaps set at 5, 
 and power upto 40% N1,
 till moving, the slowly build up to 94% N1 V1 
 rotate usally at around 155 knots 
  </line>
  <line>Maximum Flap Operating Speeds</line>
  <line>1 = 230 kn</line>
  <line>2 = 230 kn</line>
  <line>5 = 225 kn</line>
  <line>10 =210 kn</line>
  <line>15 =195 kn</line>
  <line>25 = 190 kn</line>
  <line>30 = 185 kn</line>
  <line>40 = 158 kn</line>
 
   <key>
    <name>Ctrl-b</name>
    <desc>Toggle speedbrakes</desc>
   </key>
     <key>
    <name>Tab-key</name>
    <desc>selects Autobrake-setting</desc>
   </key>
   <key>
    <name>Delete</name>
    <desc>Thrust Reverser</desc>
   </key>
 </help> 
 
 <menubar>
   <default>
    <menu n="5">

     <item n="10">
      <label>Pushback</label>
      <name>pushback</name>
      <binding>
       <command>dialog-show</command>
       <dialog-name>pushback</dialog-name>
      </binding>
     </item>

    </menu>

    <menu n="10">
     <label>Boeing 737-300</label>
     <enabled type="bool">true</enabled>

     
     <item>
      <label>Select Livery</label>
      <binding>
       <command>nasal</command>
       <script>aircraft.livery.dialog.open()</script>
      </binding>
     </item>
     <item>
	<label>Pushback</label>
      <name>pushback</name>
      <binding>
       <command>dialog-show</command>
       <dialog-name>pushback</dialog-name>
      </binding>
     </item>
    </menu>
   </default>
  </menubar>
 
  </sim>
  
 
 
  
	<consumables>
		<fuel>
			<tank n="0">
				<level-gal_us archive="y">1540</level-gal_us>
			</tank>
			<tank n="1">
				<level-gal_us archive="y">1540</level-gal_us>
			</tank>
			<tank n="2">
				<level-gal_us archive="y">2275</level-gal_us>
			</tank>
		</fuel>
	</consumables>

	<engines>
		<engine n="0">
			<running>true</running>
		</engine>
		<engine n="1">
			<running>true</running>
		</engine>
	</engines>

  <input>
    <keyboard>
      <key n="127">
	<name>Delete</name>
	<desc>Toggle Reversers</desc>
	<binding> <command>nasal</command>
	<script>reversethrust.togglereverser()</script>
	</binding>
</key> 
   

 
<key n="9">
    <name>Tab</name>
    <desc>switch autobrake</desc>
    <binding>
     <command>property-cycle</command>
     <property>/controls/gear/autobrakes</property>
     <value>0</value> 
     <value>1</value> 
     <value>2</value> 
     <value>3</value> 
     <value>4</value> 
     <value>5</value> 
    </binding>
   </key>
      
    </keyboard>
  </input>
  


	<controls>
		<gear>
			<autobrakes type="int">5</autobrakes>
			<!-- 0=off, 1=1, 2=2, 3=3, 4=MAX, 5=RTO -->
		</gear>
		<flight>
			<autospeedbrakes-armed type="string">true</autospeedbrakes-armed>
		</flight>
	

<switches>
<landing-light-l type = "bool">true</landing-light-l> 
<landing-light-r type = "bool">true</landing-light-r> 
<taxi-light>0</taxi-light>
<inboard-lights>0</inboard-lights>
<outboard-lights>0</outboard-lights>
<nav-lights>0</nav-lights>
<beacon>1</beacon>
<strobe>1</strobe>
<logo-lights>1</logo-lights>
<cabin-lights>0</cabin-lights>
<map-lights>0</map-lights>
<master-panel>0</master-panel>
<instruments-lights>0</instruments-lights>
<seatbelt-sign>0</seatbelt-sign>
</switches>
</controls>

<gear>
		<gear n="0">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
			<caster-angle-deg-damped type="double">0</caster-angle-deg-damped>
		</gear>
		<gear n="1">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
		</gear>
		<gear n="2">
			<tyre-smoke type="int">0</tyre-smoke>
			<sprayspeed-ms type="double">0</sprayspeed-ms>
			<spray-density type="double">0</spray-density>
		</gear>
		<!--<tailhook>
			<position-norm type="double">0</position-norm>
		</tailhook>-->
	</gear>

<instrumentation>
<mk-viii>
            <serviceable>true</serviceable>
            <configuration-module>
            <category-1>1</category-1><!-- GA Fast, 150' Mode 4B -->
            </configuration-module>
            <inputs>
                <arinc429>
                <decision-height-ncd>false</decision-height-ncd>
                <decision-height>200</decision-height>
                </arinc429>
            </inputs>
        </mk-viii>
 <wxradar>
 <serviceable>true</serviceable>
  <range type="int" archive="y">20</range>
  <minimized type="bool" archive="y">false</minimized>
  <switch archive="y">off</switch>
  <mode archive="y">wx</mode>
  <lightning type="bool" archive="y">false</lightning>
  <display-mode archive="y">arc</display-mode>
 </wxradar>
 	<radar>
		<range type="int" archive="y">20</range>
	</radar>
<turn-indicator>
<serviceable type="bool">true</serviceable>
<spin type="double">1</spin>
</turn-indicator>
</instrumentation>



<nasal>
    <Boeing-737-300>
             <file>Aircraft/737-300/Nasal/liveries.nas</file>      
</Boeing-737-300>

<reversethrust>
   <file>Aircraft/747-400/Nasal/reverse-thrust.nas</file>
  </reversethrust>

<airground>
<file>Aircraft/737-300/Systems/air-ground.nas</file>
</airground>
   

		<tyresmoke>
			<file>Aircraft/737-300/Nasal/tyresmoke.nas</file>
		</tyresmoke>
		
	
   
  </nasal>
</PropertyList>
