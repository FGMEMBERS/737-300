<!-- 777-300
George Luiz
-->

<PropertyList>

  <!-- Chocks -->

  <model>
    <name>Chocks</name>
    <path>Aircraft/737-300/Models/Services/Chocks/chocks.xml</path>
    <condition>
      <equals>
        <property>controls/switches/chocks</property>
        <value type="bool">true</value>
      </equals>
      <less-than>
        <property>velocities/airspeed-kt</property>
        <value>15</value>
      </less-than>
    </condition>
    <offsets>
      <x-m>0.00</x-m>
      <y-m>0.00</y-m>
      <z-m>0.00</z-m>
    </offsets>
  </model>

<!-- Engine Safety Cones -->

  <model>
    <name>Cone</name>
    <path>Aircraft/737-300/Models/Services/Safety-Cones/safety-cones.xml</path>
