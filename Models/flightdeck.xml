 <PropertyList>
  <path>flightdeck.ac</path>
 
  
  <!--cockpit-->
 <!--overhead panel-->
 <model>
    <name>ovhp</name>
    <path>Aircraft/737-300/Overhead/oh.xml</path>
    <offsets>
      <x-m>-6.55</x-m>
      <y-m>0.0</y-m>
      <z-m>1.340</z-m>
      <pitch-deg>-25</pitch-deg>
    </offsets>
  </model>

  
 <!--  Upper Panel Autopilot Controls-->
 
  <animation>
   <type>textranslate</type>
   <object-name>APias.100</object-name>
