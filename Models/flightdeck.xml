   <animation>
   <type>select</type>
   <object-name>APvs.minus2</object-name>
   <condition>
   <less-than>
   <property>/autopilot/settings/vertical-speed-fpm</property>
   <value>-990</value>
   </less-than>
   </condition>
 </animation>


   <animation>
   <type>select</type>
   <object-name>APvs.minus1</object-name>
   <condition>
<and>
   <less-than>
   <property>/autopilot/settings/vertical-speed-fpm</property>
   <value>0</value>
   </less-than>
   <greater-than>
   <property>/autopilot/settings/vertical-speed-fpm</property>
   <value>-1000</value>
   </greater-than>
</and>
   </condition>
 </animation>

   <animation>
   <type>select</type>
   <object-name>APvs.10000</object-name>
   <condition>
   <greater-than>
   <property>/autopilot/settings/vertical-speed-fpm</property>
   <value>9900</value>
   </greater-than>
   </condition>
 </animation>
 

