<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
  <path>fuel_panel.ac</path>

  <!-- Left Afterward fuelpump -->
  <animation>
    <type>rotate</type>
    <object-name>SwitchLeftAft</object-name>
    <property>b733/instrumentation/fuel/fuelpumps/leftAft</property>
    <offset-deg>-25</offset-deg>
    <factor>40</factor>
    <center>
      <x-m>-0.05337</x-m>
      <y-m>-0.106567</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>SwitchLeftAft</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>b733/instrumentation/fuel/fuelpumps/leftAft</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>b733/instrumentation/fuel/LowPress1A</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/fuel/tank[0]/pump-aft</property>
      </binding>
    </action>
  </animation>

