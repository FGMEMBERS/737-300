  <!-- Left Engine Starter -->

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>LEngineStart</object-name>
    <action>
      <name>Engine Start 1</name>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <!-- Adding condition -->
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/starter</property>
        <step>1</step>
        <min>0></min>
        <max>1</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>LEngineStart</object-name>
    <property>controls/engines/engine[0]/starter</property>
    <factor>20</factor>
    <offset-deg>0</offset-deg>
    <center>
      <x-m>0.002871</x-m>
      <y-m>0.006992</y-m>
      <z-m>0.002327</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
