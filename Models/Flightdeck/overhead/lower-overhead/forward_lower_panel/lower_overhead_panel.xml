<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

  <path>forward_lower_panel.ac</path>

  <!-- Outboard Left Landing Light -->

  <animation>
    <type>rotate</type>
    <object-name>OutbordLLandingLight</object-name>
    <property>/controls/lighting/landing-lights-fixed-left</property>
    <offset-deg>-25</offset-deg>
    <factor>40</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>OutbordLLandingLight</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/landing-lights-fixed-left</property>
      </binding>
    </action>
  </animation>


