<!-- Toggle switch with lamps -->
	<animation>
	<type>pick</type>
	<object-name>Switch</object-name>
	<visible>true</visible>
	<action>
	<button>0</button>
	<repeatable>false</repeatable>
	<binding>
	<command>property-toggle</command>
	<property>/b733/instrumentation/pedestal/com2/switch</property>
	</binding>
	<binding>
        <command>nasal</command>
              <script>com.changeActive(2);</script>
        </binding>
	</action>
	</animation>
	
	<animation>
	<type>rotate</type>
	<object-name>Switch</object-name>
	<property>/b733/instrumentation/pedestal/com2/switch</property>
	<factor>90</factor>
	<axis>
	<x>0</x><y>1</y><z>0</z>
    </axis>
	</animation>	
	
<animation>
  <type>select</type>
    <object-name>LampLeftOn</object-name>
	<object-name>LampRightOff</object-name>
  <condition>
    <equals>
      <property>b733/instrumentation/pedestal/com2/switch</property>
	  <value type="double">0</value>
    </equals>
  </condition>
</animation>

<animation>
  <type>select</type>
    <object-name>LampLeftOff</object-name>
	<object-name>LampRightOn</object-name>
  <condition>
    <equals>
      <property>b733/instrumentation/pedestal/com2/switch</property>
	  <value type="double">1</value>
    </equals>
  </condition>
</animation>

</PropertyList>
