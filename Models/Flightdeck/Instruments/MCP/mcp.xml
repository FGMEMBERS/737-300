<!-- SPEED CONTROL                                                 -->
<!-- =============================================================  --> 



<!-- ATArm Switch -->

  <animation>
  <type>pick</type>
  <visible>true</visible>
  <object-name>ATArm</object-name>
  <action>
    <name>AT Arm Switch</name>
   <button>0</button>
   <repeatable>false</repeatable> 
   <binding>
    <command>property-toggle</command>
    <property>/autopilot/internal/SPD</property>
		<step>1</step>
		<min>0></min>
		<max>1</max>
		<wrap>true</wrap>
   </binding>
  </action>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>ATArm</object-name>
   <property>/autopilot/internal/SPD</property>
   <factor>-30</factor>
	 <offset-deg>30</offset-deg>
   <center>
     <x-m>-0.00019</x-m>
     <z-m>0.05404</z-m>
   </center>
   <axis>
     <y>1</y>
   </axis>
 </animation>

<!-- MACH Sel -->

  <animation>
  <type>pick</type>
  <visible>true</visible>
  <object-name>IAS-SEL</object-name>
  <action>
    <name>MACH Sel Switch</name>
   <button>0</button>
   <repeatable>false</repeatable> 
<!--   <binding>
    <command>property-toggle</command>
